  <div>
    <label>Title</label>
    <input type="text" name="title" value="<%= song.title %>">
  </div>
  <div>
    <label>Artist</label>
    <select name="artist">
    <% artists.forEach(artist => { %>
      <% if (artist.id === song.artist) { %>
        <option selected label="<%= artist.name %>" value="<%= artist.id %>">
      <% } else { %>
        <option label="<%= artist.name %>" value="<%= artist.id %>">
      <% } %>
    <% }) %>
    </select>
  </div>
  <div>
    <label>Publish Date</label>
    <input type="date" name="publishDate"
          value="<%= song.publishDate == null ? '' : song.publishDate.toISOString().split('T')[0] %>">
  </div>
  <div>
    <label>Page Count</label>
    <input type="number" name="pageCount" min="1" value="<%= song.pageCount %>">
  </div>
  <div>
    <label>Youtube URL</label>
    <input type="text" name="ytURL" value="<%= song.ytURL %>">
  </div>
  </div>
  <div>
    <label>Cover</label>
    <input type="file" name="cover" class="filepond">
  </div>
  <div>
    <label>Description</label>
    <textarea name="description"><%= song.description %></textarea>
  </div>